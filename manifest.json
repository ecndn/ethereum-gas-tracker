{
    "manifest_version": 2,
    "name": "Ethereum Gastracker Extensions",
    "version": "1.0",
    "description": "Google Chrome Ethereum Gas Tracker Extension",
    "permissions": ["http://*/*", "https://*/*"],
    "browser_action": {
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      },
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
  